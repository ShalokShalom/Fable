{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/bin": true,
        "**/obj": true,
        "**/.fake": true,
        "**/packages": true,
        "**/node_modules": true,
        "**/bower_components": true
    },
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "gitlens.codeLens.enabled": false,
    "FSharp.excludeProjectDirectories": [
        ".git",
        "paket-files",
        "fable_modules",
        "packages",
        "node_modules",
        "src/fcs-fable"
    ],
    "editor.inlayHints.enabled": "offUnlessPressed"
}
